{
  "name": "pgats-automacao-api-trabalho-final",
  "version": "1.0.0",
  "description": "REST + GraphQL API for car rental system with JWT authentication",
  "repository": {
    "type": "git",
    "url": "https://github.com/QAkarotto/pgats-automacao-api-trabalho-final.git"
  },
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "NODE_ENV=test npx mocha 'tests/**/*.test.js' --recursive --timeout 5000 --exit --reporter spec",
    "test:unit": "NODE_ENV=test mocha tests/unit/**/*.test.js --recursive --timeout 5000 --exit --reporter spec",
    "test:integration": "NODE_ENV=test mocha tests/integration/**/*.test.js --recursive --timeout 5000 --exit --reporter spec",
    "test:external": "NODE_ENV=test mocha tests/external/**/*.test.js --recursive --timeout 5000 --exit --reporter spec",
    "test:watch": "NODE_ENV=test mocha tests/**/*.test.js --recursive --timeout 5000 --watch --reporter spec",
    "test:coverage": "NODE_ENV=test nyc --reporter=html --reporter=text --reporter=lcov mocha tests/**/*.test.js --recursive --timeout 5000 --exit --reporter spec",
    "test:coverage:json": "NODE_ENV=test nyc --reporter=json mocha tests/**/*.test.js --recursive --timeout 5000 --exit --reporter spec",
    "test:ci": "NODE_ENV=test nyc --reporter=html --reporter=text --reporter=lcov --reporter=json mocha tests/**/*.test.js --recursive --timeout 5000 --exit --reporter mochawesome",
    "lint": "echo 'No linting configured yet'",
    "lint:fix": "echo 'No linting configured yet'",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix"
  },
  "keywords": [
    "car-rental",
    "api",
    "rest",
    "graphql",
    "authentication",
    "jwt",
    "express",
    "apollo"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@graphql-tools/schema": "^10.0.25",
    "apollo-server-express": "^3.13.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^9.0.20",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "graphql-request": "^7.2.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
